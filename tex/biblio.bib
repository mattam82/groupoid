	Journal = {Functional and Logic Programming},
	Pages = {114--129},
	Pdf = {http://www.irisa.fr/vertecs/Publis/Ps/2006-FLOPS.pdf},
	Title = {{D}efining and {R}easoning {A}bout {R}ecursive {F}unctions: {A} {P}ractical {T}ool for the {C}oq {P}roof {A}ssistant},
